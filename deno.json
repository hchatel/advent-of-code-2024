{
    "tasks": {
        "new": "cp -r template",
        "run": "sh utils/denoTaskRun.sh"
    },
    "fmt": {
        "indentWidth": 4,
        "lineWidth": 120
    },
    "lint": {
        "rules": {
            "exclude": [
                "no-unused-vars"
            ]
        }
    },
    "test": {
        "exclude": [
            "template/"
        ]
    },
    "imports": {
        "@std/assert": "jsr:@std/assert@1"
    }
}
